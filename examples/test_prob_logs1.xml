<?xml version="1.0" ?>
<!-- This model specification is used in the script APS_define_probability_logs
     Conditional probabilities are used in this example.
     The facies names defined in input facies log are A, B, C, D
     For each zone, the modelled facies are F1,F2,F3
-->
<ProbLogs>
  <GridModelName>GridModelFine</GridModelName>
  <BlockedWells> BW4  </BlockedWells>
  <FaciesLogName> Facies </FaciesLogName>
  <ZoneLogName>   Zone  </ZoneLogName>
  <OutputPrefix> Prob1 </OutputPrefix>
  <ModellingFaciesPerZone>
    <Zone number="1">  F1 F2 F3 </Zone>
    <Zone number="2">  F1 F2 F3 </Zone>
    <Zone number="3">  F1 F2 F3 </Zone>
    <Zone number="4">  F1 F2 F3 </Zone>
    <Zone number="5">  F1 F2 F3 </Zone>
    <Zone number="6">  F1 F2 F3  </Zone>
  </ModellingFaciesPerZone>
  <UseConditionalProbabilities> 1 </UseConditionalProbabilities>
  <CondProbMatrix number="1">
    <Line>  F1  A  1.0 </Line>
    <Line>  F2  A  0.0 </Line>
    <Line>  F3  A  0.0 </Line>

    <Line>  F1  B  0.0 </Line>
    <Line>  F2  B  1.0 </Line>
    <Line>  F3  B  0.0 </Line>

    <Line>  F1  C  0.0 </Line>
    <Line>  F2  C  0.0 </Line>
    <Line>  F3  C  1.0 </Line>

    <Line>  F1  D  0.7 </Line>
    <Line>  F2  D  0.2 </Line>
    <Line>  F3  D  0.1 </Line>
  </CondProbMatrix>
  <CondProbMatrix number="2">
    <Line>  F1  A  1.0 </Line>
    <Line>  F2  A  0.0 </Line>
    <Line>  F3  A  0.0 </Line>

    <Line>  F1  B  0.0 </Line>
    <Line>  F2  B  1.0 </Line>
    <Line>  F3  B  0.0 </Line>

    <Line>  F1  C  0.0 </Line>
    <Line>  F2  C  0.0 </Line>
    <Line>  F3  C  1.0 </Line>
  </CondProbMatrix>
  <CondProbMatrix number="3">
    <Line>  F1  A  1.0 </Line>
    <Line>  F2  A  0.0 </Line>
    <Line>  F3  A  0.0 </Line>

    <Line>  F1  B  0.0 </Line>
    <Line>  F2  B  1.0 </Line>
    <Line>  F3  B  0.0 </Line>

    <Line>  F1  C  0.0 </Line>
    <Line>  F2  C  0.0 </Line>
    <Line>  F3  C  1.0 </Line>
  </CondProbMatrix>
  <CondProbMatrix number="4">
    <Line>  F1  A  1.0 </Line>
    <Line>  F2  A  0.0 </Line>
    <Line>  F3  A  0.0 </Line>

    <Line>  F1  B  0.0 </Line>
    <Line>  F2  B  1.0 </Line>
    <Line>  F3  B  0.0 </Line>

    <Line>  F1  C  0.0 </Line>
    <Line>  F2  C  0.0 </Line>
    <Line>  F3  C  1.0 </Line>
  </CondProbMatrix>
  <CondProbMatrix number="5">
    <Line>  F1  A  1.0 </Line>
    <Line>  F2  A  0.0 </Line>
    <Line>  F3  A  0.0 </Line>

    <Line>  F1  B  0.0 </Line>
    <Line>  F2  B  1.0 </Line>
    <Line>  F3  B  0.0 </Line>

    <Line>  F1  C  0.0 </Line>
    <Line>  F2  C  0.0 </Line>
    <Line>  F3  C  1.0 </Line>
  </CondProbMatrix>
  <CondProbMatrix number="6">
    <Line>  F1  A  1.0 </Line>
    <Line>  F2  A  0.0 </Line>
    <Line>  F3  A  0.0 </Line>

    <Line>  F1  B  0.0 </Line>
    <Line>  F2  B  1.0 </Line>
    <Line>  F3  B  0.0 </Line>

    <Line>  F1  C  0.0 </Line>
    <Line>  F2  C  0.0 </Line>
    <Line>  F3  C  1.0 </Line>

    <Line>  F1  D  0.7 </Line>
    <Line>  F2  D  0.2 </Line>
    <Line>  F3  D  0.1 </Line>
  </CondProbMatrix>
</ProbLogs>
