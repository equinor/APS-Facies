{
  "name": "aps-gui",
  "version": "1.5.0",
  "private": true,
  "description": "The GUI component of the APS workflow",
  "author": "Sindre Nistad <snis@equinor.com>",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "lint": "vite lint",
    "serve:api": "make -C ../.. run-rms.uipy-mock",
    "serve:gui": "vite --host 0.0.0.0",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.9.0",
    "fast-xml-parser": "^4.3.4",
    "floating-vue": "^5.2.2",
    "hex-rgb": "^5.0.0",
    "lodash": "^4.17.11",
    "markdown-it": "^14.1.0",
    "markdown-it-footnote": "^3.0.2",
    "mathjs": "^12.1.0",
    "object-hash": "^2.0.1",
    "pinia": "^2.1.7",
    "plotly.js-dist-min": "^2.29.1",
    "roboto-fontface": "*",
    "uuid": "^8.1.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vue": "^3.4.20",
    "vue-swatches": "^2.1.1",
    "vuetify": "^3.5.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.196",
    "@types/markdown-it": "^12.2.3",
    "@types/object-hash": "^2.2.1",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^0.28.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "eslint-plugin-vuetify": "^2.1.1",
    "ify-loader": "^1.1.0",
    "improved-yarn-audit": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^11.2.6",
    "sass": "^1.67.0",
    "terser": "^5.31.0",
    "ts-custom-error": "^3.3.1",
    "typescript": "^5.3.3",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.6.4",
    "vue-eslint-parser": "^9.4.2",
    "vue-meta": "^2.4.0",
    "vue-tsc": "^1.8.27"
  },
  "postcss": {
    "plugins": {}
  },
  "browserslist": [
    "chrome >= 56"
  ],
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "preset": "ts-jest/presets/js-with-ts",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "node",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "resolver": "jest-webpack-resolver",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!@vue|(?!deck.gl)|ng-dynamic)"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)",
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ],
    "collectCoverage": true
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint"
    ]
  }
}
